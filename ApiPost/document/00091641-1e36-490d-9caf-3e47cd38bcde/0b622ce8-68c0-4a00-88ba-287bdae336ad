    <script type="text/javascript">
        template.defaults.imports.marked = function (md) {
            marked.setOptions({
                renderer: new marked.Renderer(),
                highlight: function (code) {
                    return hljs.highlight('javascript', code).value;
                },
                breaks:true
            });
            return marked(md).replace(/<pre>/g, "<pre class='hljs javascript'>");
        }

        $(function () {
            // 加载默认文档
                            WIKI.get('0b622ce8-68c0-4a00-88ba-287bdae336ad').then(function(data){
                    let html = template("template-markdown-tpl", data);
                    $('.markdown').html(html);

                    let _navs = [];
                    $(".markdown-section h2, .markdown-section h3").each(function(){
                         _navs.push({
                            text:$(this).text(),
                            type:$(this)[0].tagName,
                            id:$(this).attr('id')
                        });
                    });

                    if(_navs.length > 0){
                        $('.float-nav').html(template("template-float-nav-tpl", {list:_navs})).show();
                    }else{
                        $('.float-nav').hide();
                    }
                });
                        //左侧菜单
            var h = 0;
            $(document).on("click", ".menu-item-parent .sub-title", function (e) {
                if (e && e.preventDefault) {
                    e.preventDefault();
                } else {
                    window.event.returnValue = false;
                }

                $(this).parent().toggleClass('active collapse').find('ul').toggle();
            })

            // 加载单个文档
            $(document).on("click", ".sub-title-link:not('.index-link')", function (e) {
                let _this = this;

                if (e && e.preventDefault) {
                    e.preventDefault();
                } else {
                    window.event.returnValue = false;
                }

                let document_id = $(_this).attr('document_id')
                
                WIKI.get(document_id).then(function(data){
                    // console.log(data)
                    if (!data.errcode) {
                        let html = template("template-markdown-tpl", data);
                        $('.markdown').html(html);
                        $('.menu-group .sub-title-link').removeClass('active')
                        $(_this).addClass('active');

                        window.history.pushState({}, 0, $(_this).prop('href'));

                        let _navs = [];
                        $(".markdown-section h2, .markdown-section h3").each(function(){
                            _navs.push({
                                text:$(this).text(),
                                type:$(this)[0].tagName,
                                id:$(this).attr('id')
                            });
                        });

                        if(_navs.length > 0){
                            $('.float-nav').html(template("template-float-nav-tpl", {list:_navs})).show();
                        }else{
                            $('.float-nav').hide();
                        }
                    }
                });

                //$(".markdown-section h2, .markdown-section h3").each(function(){console.log($(this).text())})
            })

            //图片放大
            $(document).on('click', 'img', function () {
                let img = $(this).attr('src');
                $('.imgtc').show().find('img').attr('src', img);
            })

            $('.imgtc').click(function () {
                $(this).toggle();
            })
        })
    </script>
